{
  "compileOnSave": true,
  "include": ["./"],
  "compilerOptions": {
    "baseUrl": ".",
    "sourceMap": false,
    "declaration": false,
    "target": "es2015",
    "lib": ["dom", "es2017"],
    "outDir": "../dist",
    "rootDir": ".",
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "ES2020"
  }
}
